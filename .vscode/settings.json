{
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.fixAll": "explicit"
  },
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "files.associations": {
    "CODEOWNERS": "plaintext",
    "LICENSE": "plaintext",
    "**/layouts/**/*.xml": "plaintext"
  },
  "search.exclude": {
    "**/vendor": true
  },
  "i18n-ally.localesPaths": [
    "themes/2345lgbt/i18n"
  ]
}
